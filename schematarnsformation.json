{
    "dataFlows": [
        {
            "streams": [
                "CustomLogs"
            ],
            "destinations": [
                "logAnalytics"
            ],
            "transformKql": "parse kind=regex EventData with timestamp '[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}' level ' - ' message '*' | extend TimeGenerated = parse_datetime(timestamp, 'yyyy-MM-dd HH:mm:ss')",
            "outputSchema": {
                "schemaVersion": 1,
                "columns": [
                    {
                        "name": "TimeGenerated",
                        "type": "DateTime"
                    },
                    {
                        "name": "level",
                        "type": "String"
                    },
                    {
                        "name": "message",
                        "type": "String"
                    }
                ]
            },
            "destinationTable": "MyUdacity_CL"
        }
    ]
}
